MainSection:NewToggle("Dupe Method", "Toggle On Then Withdraw Huges From Bank", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/PsxDupeOp/DupeScriptPSX/main/PsxDupe.lua"))()    else
        print("Toggle Off")

    end
end)
